
this is card model


export interface Card{
    cardId:number;
    authorId:number;
    authorName:string;
    title:string;
    description:string;
    tags:string[];
    date:string;
    likes:number;
}


this is the user model 

export interface User{
    id:number;
    name:string;
    email:string;
    password:string;
    followingIds:number[];
    favoriteCardIds:number[];
}


