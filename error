string temp(int n){
        if(n==1){
            return "1";
        }
        string s=temp(n-1);
        int count=0;
        char number=0;
        string s1="";
        for(int i=1;i<=s.size();i++){
            if(i==1){
                number=s[i];
                count++;
            }else if(s[i]==s[i-1]){
                count++;
            }else{
                s1+=to_string(count);
                s1+=number;
                count=1;
                number=s[i];
            }
        }
        return s;
    }
